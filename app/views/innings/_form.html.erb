<%= simple_form_for(@innings, :url => match_innings_path(@match, @innings)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :matchname %>
    <%#= f.input :date %>
    <%= f.input :inningsname, label: "Innings name (e.g., South Africa 1st Innings)" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
