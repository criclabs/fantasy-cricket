<% provide(:title, "#{@innings.inningsname}") %>
<p id="notice"><%= notice %></p>

<h1>
	<%= @innings.matchname %>
	<small>
		<%= @innings.date %>
	</small>
</h1>
<h2><%= @innings.inningsname %></h2>

<%= render "player_scores/index" %>

<% if admin_user? %>
  <%= link_to 'Edit', edit_match_innings_path(@match, @innings), class: "btn btn-small" %>
<% end %>
<%= link_to 'Back', match_innings_index_path(@match), class: "btn btn-small" %>
<% if admin_user? %>
  <%= link_to 'New Player score', new_match_player_score_path, class: "btn btn-small" %>
<% end %>
